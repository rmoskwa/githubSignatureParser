# Copy this file to .env and fill in your values

# Required: Google Gemini API key for LLM enhancement
GEMINI_API_KEY=your_gemini_api_key_here

# Required: Supabase configuration for database storage
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Optional: Path to MATLAB functions directory
# If not set, will need to use --path argument
# MATLAB_FUNCTIONS_PATH=/path/to/matlab/functions

# Optional: Set log level (DEBUG, INFO, WARNING, ERROR)
# LOG_LEVEL=INFO